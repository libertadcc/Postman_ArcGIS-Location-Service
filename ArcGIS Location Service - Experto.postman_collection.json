		{
			"name": "Get access token",
			"item": [
				{
					"name": "Get ArcGIS Online Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = JSON.parse(responseBody);\r",
									"tests[\"Access token is present\"] = responseBody.has(\"token\");\r",
									"\r",
									"postman.setEnvironmentVariable(\"token\", jsonData.token);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "username",
									"value": "{{username}}",
									"type": "text"
								},
								{
									"key": "password",
									"value": "{{password}}",
									"type": "text"
								},
								{
									"key": "referer",
									"value": "arcgis.com",
									"type": "text"
								},
								{
									"key": "f",
									"value": "json",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "https://www.arcgis.com/sharing/rest/generateToken",
							"protocol": "https",
							"host": [
								"www",
								"arcgis",
								"com"
							],
							"path": [
								"sharing",
								"rest",
								"generateToken"
							]
						}
					},
					"response": []
				}
			]
		},
